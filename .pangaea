environment: development
# provider: {vagrant, gce}
provider: vagrant
compiler:
  default_copy: true
  targets:
    - pangaea
    - config
    - app/*/kubefiles
    - app/secrets
compiler:
  targets:
    - path: pangaea
      to: pangaea
      filename_template: "pan.%f.%g"
kubernetes:
  dns_domain: kubeapp
  dns_server_ip: 10.0.0.10
coreos:
  docker_gcr_credentials: true
gce:
  instance_name: testinstance
