environment: development
# provider: {vagrant, gce}
provider: vagrant
compiler:
  targets:
    - path: pangaea
      to: pangaeac
    - path: config
    - path: app/*/kubefiles
      to: pangaeac
    - path: app/sec*
      to: pangaeac

kubernetes:
  dns_domain: kubeapp
  dns_server_ip: 10.0.0.10
coreos:
  docker_gcr_credentials: true
gce:
  instance_name: testinstance
